{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": [0, { "ignorePureComponents": true }],
    "no-param-reassign": [2, { "props": false }],
    "comma-dangle":[2,"never"]
  },
  "env": {
    "browser": true,
    "node": true
  }
}